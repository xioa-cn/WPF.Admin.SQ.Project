<xservices:RegisterPage
    x:Class="PressMachineMainModeules.Views.AutoMesTemplateView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:i18n="clr-namespace:WPF.Admin.Themes.I18n;assembly=WPF.Admin.Themes"
    xmlns:components="clr-namespace:PressMachineMainModeules.Components"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:local="clr-namespace:PressMachineMainModeules.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:xservices="clr-namespace:WPF.Admin.Service.Common;assembly=WPF.Admin.Service"
    Title="AutoMesTemplateView"
    mc:Ignorable="d">
    <hc:DialogContainer>
        <Border
            Margin="1"
            BorderBrush="{DynamicResource Border.Brush}"
            BorderThickness="1">
            <Grid>
                <TabControl>
                    <TabItem Header="{i18n:Localize Detection.Front_EndDetection}">
                        <components:AutoMesCheckedCodeView />
                    </TabItem>
                    <TabItem Header="{i18n:Localize Detection.DataUpload}">
                        <components:AutoMesUploadDatView />
                    </TabItem>
                </TabControl>
            </Grid>
        </Border>
    </hc:DialogContainer>
</xservices:RegisterPage>